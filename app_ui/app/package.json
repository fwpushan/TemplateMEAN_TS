{
  "name": "freshworks-react-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for React.js projects",
  "private": true,
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "autoprefixer": "^8.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "3.6.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.17.4",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "4.18.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.2"
  },
  "optionalDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "jest-junit": "^3.7.0",
    "puppeteer": "^1.4.0"
  },
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 PORT=3000 webpack-dev-server --hot --inline --env development --host 0.0.0.0 --port 3000",
    "build": "webpack --env production",
    "set-env": "node scripts/generateClientEnv.js",
    "serve": "npm run set-env && node server.js",
    "ci-audit": "npm audit | tail -4 > audit-report.txt",
    "lint": "eslint .",
    "ci-test": "CI=true jest -u --env=jsdom --testResultsProcessor=jest-junit --coverage --testPathIgnorePatterns .e2e",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --coverage --testResultsProcessor=jest-junit"
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.js"
    ],
    "moduleNameMapper": {
      "^@babel(.*)$": "<rootDir>/node_modules/@babel$1",
      "^@(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/mocks/fileMock.js",
      "\\.(scss|less)$": "<rootDir>/src/tests/mocks/styleMock.js"

    }
  },
  "jest-junit": {
    "output": "./coverage/tests-report.xml"
  },
  "browserslist": [
    "> 0.1%"
  ]
}
